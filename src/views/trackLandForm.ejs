                                        <div class="card-body">
                                            <h3>Bids Placed</h3>
                                            <h1 class="display-4">
                                                <i class="fa fa-pencil"></i> 6
                                            </h1>
                                            <a href="/home/viewBidsPlaced" class="btn btn-outline-light btn-sm">View</a>
                                        </div>
                                    </div>
                
