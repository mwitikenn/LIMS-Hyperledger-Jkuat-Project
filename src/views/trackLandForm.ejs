                                                <i class="fa fa-folder-open-o"></i> 4
                                            </h1>
                                            <a href="/home/viewMyOffers" class="btn btn-outline-light btn-sm">View</a>
                                        </div>
                                    </div>
                
                                </div>
                
                                <div class="col-md-7">
                                    <div class="card">
                                        <div class="card-header">
                                            <h4>My Land Assets</h4>
                                        </div>
                                        <table id="dtDynamicVerticalScrollExample" class="table table-striped table-bordered table-wrapper-scroll-y my-custom-scrollbar ">
                                            <thead class="thead-inverse">
                                                <tr>
                                                    <th>Deed Number</th>
                                                    <th>Land Value</th>
                                                    <th>Land Type</th>
                                                    <th>Size</th>
                                                    <th>Bid Status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for(var i=0; i<parcelData.length; i++){%>
                                                <tr>
                                                    <td ><%= parcelData[i].deedno %></td>
                                                    <td ><%= parcelData[i].landValue %></td>
                                                    <td ><%= parcelData[i].typeOfLand %></td>
                                                    <td ><%= parcelData[i].size %></td>
                                                    <td ><%= parcelData[i].bidstatus %></td>
