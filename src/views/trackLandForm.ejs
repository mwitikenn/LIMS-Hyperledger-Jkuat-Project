                                        <label class="control-label">Offer ID</label>
                                        <div>
                                            <input type="text" class="form-control input-lg" name="offerID" value="" required>
                                        </div>
                                    </div>
                                    <div> <button type="submit" class="btn btn-success">Create offer</button>
                                    </div>
